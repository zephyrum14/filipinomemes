mga_persona = [
  { pangalan: 'Gener', kasarian: 'lalaki' },
  { pangalan: 'Karmen', kasarian: 'babae' }
]

mga_persona.its adbans |tao|
  payless tao[:kasarian] == 'lalaki'
    panomonasabe "si #{tao[:pangalan]} ay babae!"
  ginagawa_mue
    panomonasabe "si #{tao[:pangalan]} ay lalaki!"
  finish_na
finish_na
