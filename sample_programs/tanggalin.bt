grupo Hayop
  bilang Aso
    ang tahol
      mag_print 'baw waw!'
    finish_na
  finish_na
finish_na

bilang Pusa < Hayop::Aso
  tanggalin tahol

  ang meow
    mag_print 'meow wahu!'
  finish_na
finish_na

pusa = Pusa.kumatawan

wala_na
  pusa.tahol
agapan => pagkakamali
  mag_print "ang pagkakamali ay '#{pagkakamali}'"
finish_na
